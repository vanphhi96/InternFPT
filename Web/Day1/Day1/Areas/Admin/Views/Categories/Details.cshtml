    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Details</h2>

<div>
    <h4>category</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Name
        </dt>

        <dd>
            @Model.name
        </dd>

        <dt>
           Last Update
        </dt>

        <dd>
            @Model.last_update
        </dd>

    </dl>
</div>
<p>
